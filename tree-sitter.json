{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rst",
      "camelcase": "Rst",
      "title": "ReStructuredText",
      "scope": "source.rst",
      "file-types": [
        "rst"
      ],
      "injection-regex": "^rst$",
      "class-name": "TreeSitterRst"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "reStructuredText grammar for tree-sitter",
    "authors": [
      {
        "name": "Santos Gallegos",
        "email": "stsewd@proton.me",
        "url": "https://stsewd.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/stsewd/tree-sitter-rst/",
      "funding": "https://github.com/sponsors/stsewd"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}